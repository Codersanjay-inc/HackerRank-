#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
char ch[10] = "C";
char s[10] = "Language";
char sen[20] = "Welcome To C!!";
scanf("%[^\n]%*c",ch);
scanf("%[^\n]%*c",s);
scanf("%[^\n]%*c",sen);
printf("%s\n",ch);
printf("%s\n",s);
printf("%s\n",sen);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
